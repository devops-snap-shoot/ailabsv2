{
  "name": "ionic_boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate Ionic React - NodeJS",
  "main": "index.js",
  "scripts": {
    "start-prod": "rm -r server/dist && npm run start-prod --prefix server",
    "start": "npm start --prefix server",
    "start-demo": "npm run start-demo --prefix server",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev-server": "npm run start-dev --prefix server",
    "dev-client": "npm run dev --prefix client",
    "dev-client-ailabs": "npm run ailabs --prefix client",
    "dev-client-demo": "npm run demo --prefix client",
    "dev": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm run dev-client\"",
    "dev-ailabs": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm run dev-client-ailabs\"",
    "dev-demo": "concurrently --kill-others-on-fail \"npm run dev-server\" \"npm run dev-client-demo\"",
    "install-server": "cd ./server && npm install",
    "install-client": "cd ./client && npm install",
    "build-client-prod": "cd ./client && ionic build --prod",
    "build-server-prod": "cd ./server && npm run build",
    "build-prod": "npm run build-client-prod && npm run build-server-prod"
  },
  "author": "Gonzalo Orellana <gonzalo.orellana@kauel.com>",
  "license": "ISC",
  "dependencies": {
    "@azure/storage-blob": "^12.25.0",
    "@google/generative-ai": "^0.21.0",
    "@types/nodemailer": "^6.4.4",
    "accesscontrol": "^2.2.1",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "concurrently": "^7.3.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "cron": "^3.1.7",
    "helmet": "^8.0.0",
    "hpp": "^0.2.3",
    "i18n": "^0.15.1",
    "i18next": "^23.15.1",
    "i18next-browser-languagedetector": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.7.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "nodemailer": "^6.7.2",
    "pdf-parse": "^1.1.1",
    "pdf2json": "^3.1.4",
    "react-i18next": "^15.0.2",
    "socket.io": "^4.8.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.14.2",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.16.10",
    "@types/validator": "^13.12.2",
    "@wixc3/react-board": "^4.3.2",
    "typescript": "^5.6.2"
  }
}
